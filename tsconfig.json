{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      },
    ],
    "baseUrl": ".",
    "paths": {
      "@": [
        "./"
      ],
      "@/*": [
        "./*"
      ],
      "@components": [
        "./components"
      ]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/(launchpad)/create-nft", "config/GoogleAnalytics.js", "hooks/useEthereumAddressSVG.js"],
  "exclude": ["node_modules"]
}
